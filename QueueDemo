//Basic Queue

import java.util.LinkedList;
import java.util.Queue;
public class QueueDemo {
    public static void main(String[]args){
        Queue q = new  LinkedList();
        q.offer(10);
        q.offer(20);
        q.offer(30);
        q.offer(40);

        System.out.println(q);

        System.out.println(q.poll());//first element remove
        System.out.println(q);

        System.out.println(q.peek());//head element return
        System.out.println(q.remove());//first element remove
    }
}
